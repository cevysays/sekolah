# DO NOT REMOVE
ErrorDocument 403 /403.html
ErrorDocument 404 /404.html
ErrorDocument 405 /405.html
ErrorDocument 500 /500.html
ErrorDocument 503 /503.html
# DO NOT REMOVE THIS LINE AND THE LINES ABOVE anutun:ERRORPAGEID
# Do not remove this line or mod_rewrite rules and search engine friendly URLs will stop working


RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteEngine On

# Error folder
Options -Indexes
Options +FollowSymLinks


#=========================INDEX================================================
RewriteRule ^/(.*)/?$ index.php?page=$1

# ============== ARTICLE ================================
RewriteRule ^berita-cari/?$ lib/berita-cari.php
RewriteRule ^berita/search/([A-Za-z0-9-]+)$ modules/blog-search.php?keyword=$1
RewriteRule ^berita/search/([A-Za-z0-9-]+)/([0-9]+)$ modules/blog-search.php?keyword=$1&page=$2

# article categories
RewriteRule tags/([A-Za-z0-9-]+)$ modules/blog_tags.php?tags=$1
RewriteRule berita/tags/([A-Za-z0-9-]+)/([0-9]+)$ module/blog-categories.php?tags=$1&page=$2


# detail article
RewriteRule ^post/?(.*)\.html$ modules/blog_get.php?link_article=$1
RewriteRule ^action-comment/?$ action/comment-proses.php

#======================== PAGES ===============================================
RewriteRule ^pages/?(.*)\.html$ modules/pages.php?link_pages=$1

#======================== GENDA ===============================================
RewriteRule ^agenda/?$ modules/agenda.php
RewriteRule page/(.*)/?$ modules/agenda.php?page=$1
RewriteRule ^agenda/view/?(.*)\.html$ modules/agenda_get.php?link_agenda=$1



#======================== ALMUNI ===============================================
RewriteRule ^data-alumni/?$ modules/alumni.php
RewriteRule ^data-alumni/view/?(.*)\.html$ modules/alumni_get.php?kode_alumni=$1
RewriteRule ^submit/alumni/?$ modules/submit-alumni.php

#======================== ALMUNI ===============================================
RewriteRule ^anggota/?$ modules/guru.php
RewriteRule ^anggota/view/?(.*)\.html$ modules/guru-get.php?link_guru=$1

#======================== GALERY FOTO ===============================================
RewriteRule ^galery-foto/?(.*)\.html$ modules/galery.php
#--categories
RewriteRule ^galery-foto/([A-Za-z0-9-]+)$ modules/galery_get.php?link_categories=$1
RewriteRule galery-foto/([A-Za-z0-9-]+)/([0-9]+)$ modules/galery_get.php?link_categories=$1&page=$2

#======================== DOWNLOAD ===============================================
RewriteRule ^download/?$ modules/download.php
RewriteRule ^download/files/([A-Za-z0-9-]+)$ modules/download-get.php?link_download=$1

#======================== BUKU TAMU ===============================================
RewriteRule ^buku-tamu/?$ modules/buku_tamu.php

#======================== KONTAK ===============================================
RewriteRule ^kontak-kami/?(.*)\.html$ modules/contact.php

#Blokir IP
order allow,deny
deny from 202.67.45.47
deny from 202.67.45.47
allow from all
#Blokir IP


# HEMAT BADWIN
<ifModule mod_expires.c>
ExpiresActive On
ExpiresDefault "access plus 5 seconds"
ExpiresByType image/x-icon "access plus 2592000 seconds"
ExpiresByType image/jpeg "access plus 2592000 seconds"
ExpiresByType image/png "access plus 2592000 seconds"
ExpiresByType image/gif "access plus 2592000 seconds"
ExpiresByType application/x-shockwave-flash "access plus 2592000 seconds"
ExpiresByType text/css "access plus 604800 seconds"
ExpiresByType text/javascript "access plus 216000 seconds"
ExpiresByType application/javascript "access plus 216000 seconds"
ExpiresByType application/x-javascript "access plus 216000 seconds"
ExpiresByType text/html "access plus 600 seconds"
ExpiresByType application/xhtml+xml "access plus 600 seconds"
</ifModule>
# END Expire headers


# 480 weeks 
Header set Cache-Control "max-age=290304000, public"
 
# 2 DAYS 
Header set Cache-Control "max-age=172800, public, must-revalidate" 

# 2 HOURS 
Header set Cache-Control "max-age=7200, must-revalidate"

